				<!--
				<resource>
				<directory>src/webapp/WEB-INF/</directory>
				</resource>
				<resource>
				<directory>src/webapp/WEB-INF/classes</directory>
				<includes>ehcache.xml</includes>
				</resource>
				-->
			</resources>
			<includes>
				<include>**/*Test.java</include>
			</includes>
		</unitTest>
		<resources>
			<resource>
				<directory>src/main/java</directory>
